[
    ParticleEffect {
        Class: {
            Graphic: {
                Texture: "Textures/Particles/Blood.png"
                FrameCount: 6;
                Width: 10;
                Height: 10;
                FrameLength: 100msec;
            };
            Blend: BlendState.AlphaBlend;
            Color: [
                Curve {
                    IsConstant: True;
                    PreLoop: CurveLoopType.Constant;
                    PostLoop: CurveLoopType.Constant;
                    Keys: [
                        CurveKey {
                            Continuity: CurveContinuity.Smooth;
                            Position: 0.3;
                            TangentIn: 0;
                            TangentOut: 0;
                            Value: 0.8;
                        }
                    ];
                }
                [255 255 255 255]
                [255 0 0 255]
            ];
            Speed: [{} 40 80];
            Scale: [{} 1 1];
            Angle: [{} 0 360deg];
            lifetime: 2sec;
        }
        Speed: [5 100];
        Spread: [-20deg 20deg];
        Count: [10 200];
    }
    ParticleEffect {
        Class: {
            Graphic: {
                Texture: "Textures/Particles/trail.png"
                Width: 8;
                Height: 8;
            };
            Blend: BlendState.Additive;
            Color: [
                Curve {
                    IsConstant: True;
                    PreLoop: CurveLoopType.Constant;
                    PostLoop: CurveLoopType.Constant;
                    Keys: [
                        CurveKey {
                            Continuity: CurveContinuity.Smooth;
                            Position: 0.3;
                            TangentIn: 0;
                            TangentOut: 0;
                            Value: 0.8;
                        }
                    ];
                }
                [255 255 255 255]
                [255 0 0 255]
            ];
            Speed: [{} 10 60];
            Scale: [{} 1 1];
            Angle: [{} 0 360deg];
            lifetime: [2sec 5sec];
        }
        Speed: [15 50];
        Spread: [-40deg 40deg];
        Count: [10 50];
    }
    FluidEffect {
        Class: @"Defs/Fluids/Blood.fluid.tk";
        Speed: [5 20];
        Spread: [-50deg 50deg];
        Count: [10 20];
    }
]