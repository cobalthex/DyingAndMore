EffectsClass {
	Effects: [
		ParticleEffect {
            Class: {
                Sprite: {
                    Texture: @"Effects/Trail.png"
                    Center: true;
                    Tween: TweenStyle.Overlap;
                };
                Blend: BlendState.Additive;
                InitialSpeed: [50 100];
                lifetime: [0.2sec 0.5sec];
            }
            Count: [0 3];
            Spread: [-0.9pi -1.1pi];
            Radius: 3;
        }
	]
}